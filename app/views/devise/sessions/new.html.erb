<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<div id="login">
  <div id="cap-top"></div>
  <div id="cap-body">
    <center>
    <div id="branding">
      <%= image_tag "teststub_welcome.png", :id => 'imgLogo', :title => 'VistA Novo welcomes you!', :alt => 'VistA Novo logo' %>
    </div>
    
    <div id="panelLogin" class="panel">
      <%= f.label :email %>
      <%= f.text_field :email %>
      <%= f.label :password %>
      <%= f.password_field :password %>
      
      <div class="accountError">
        <%= alert %>
      </div>

      <div class="actions">
        <%= submit_tag "Login", 'class' => "btn btn-info" %>
       
      </div>

      <%= render :partial => "devise/shared/links" %>

    </div>
    </center>
  </div>
  <div id="cap-bottom"></div>
</div><!-- END #login -->
<% end %>